v0
#include<stdio.h>
int main(void)
{
	char s[] = "3+4";
	int x;
	x = s[0] - '0';
	x += s[2] - '0';
	printf("%d\n",x);

	return 0;
}

v1
#include<stdio.h>
int main(void)
{
	char s[] = "1+1+2+3+4+4";
	int i,x;
	x = s[0] - '0';
	for(i = 1;s[i] != '\0';i++)
	{
		if(s[i] == '+')
		{
			i++;
			x += s[i] - '0';
		}
		else if(s[i] == '-')
		{
			i++;
			x = x - (s[i] - '0');
		}
	}	
	printf("%d\n",x);
	return 0;
}


v2
#include<stdio.h>
int main(void)
{
	char s[] = "1*2/2*2*4*4";
	int i,x;
	x = s[0] - '0';
	for(i = 1;s[i] != '\0';i++)
	{
		if(s[i] == '*')
		{
			i++;
			x = x*(s[i] - '0');
		}
		else if(s[i] == '/')
		{
			i++;
			x = x/(s[i] - '0');
		}
	}	
	printf("%d\n",x);
	return 0;
}

v3
#include<stdio.h>
#include<string.h>
int main(void)
{	
	char s[] = "2+2*3+2/2-1";
	char x[100] = {'\0'};
	int i,j,y = 0;;
	for(i = 0,j = 0;i < strlen(s);i++,j++)
	{
		if(s[i+1] == '*'||s[i+1] == '/')
		{
			if(s[i+1] == '*')
			{
				x[j] = (s[i] - '0')*(s[i+2] - '0') + '0';
			}
			else if(s[i+1] == '/')
			{
				x[j] = (s[i] - '0')/(s[i+2] - '0') + '0';
			}
			i += 2;
		}
		else if(s[i+1] != '*'&&s[i+1] != '/')
		{
			x[j] = s[i];	
		}
		else if(s[i+1] == '\0')
		{
			x[j] = s[i];
			x[j+1]=s[i+1];
		}
	}
	for(j = 0;j < strlen(x);j++)
	{
		if(x[j+1] == '+')
		{
			y += x[j] - '0';
		}
		else if(x[j+1] == '-')
		{
			y = y - (x[j] - '0');
		}
		else if(x[j+1] == '\0')
		{
			y += x[j] - '0';
		}
	}
	printf("%f\n",y);
	return 0;
}

